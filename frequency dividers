f/1.5 :
module f_15(input clk,rst,output reg [1:0]count,output y,reg enb1,enb2);
always@(posedge clk)begin
if(rst)
count<=0;
else if(count<2)
count<=count+1'b1;
else 
count<=0;
end
always@(posedge clk)begin
if(count==2)
enb1<=1;
else
enb1<=0;
end
always@(negedge clk)begin
if(count==1)
enb2<=1;
else
enb2<=0;
end
assign y=enb1|enb2;
endmodule

