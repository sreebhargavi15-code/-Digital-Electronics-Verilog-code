
f/3 with 50%:
module f3_with_50(input clk,rst,output reg [1:0] count,output y,reg enb1,enb2);
always@(posedge clk)begin
if(rst)
count<=0;
else if(count<2)
count<=count+1'b1;
else
count<=0;
end
always@(posedge clk)
begin
if(count==2)
enb1<=1;
else
enb1<=0;
end
always@(negedge clk)
begin
if(count==0)
enb2<=1;
else
enb2<=0;
end
assign y=enb1|enb2;
endmodule


test bench:
module f3_with_50tb;
reg clk,rst;
wire [1:0]count;
wire enb1,enb2,y;
f3_with_50 dut(clk,rst,count,enb1,enb2,y);
initial begin
{clk,rst}=0;
end
always #5 clk=~clk;
initial begin
rst=1;
#10;
rst=0;
end
endmodule


f/5 with 50%:
module f5_50(input clk,rst,output reg [2:0]count,output y,reg enb1,enb2);
always@(posedge clk)begin
if(rst)
count<=0;
else if(count<4)
count<=count+1'b1;
else
count<=0;
end
always@(posedge clk)begin
if(count==4||count<1)
enb1<=1;
else 
enb1<=0;
end
always@(negedge clk)begin
if(count==1)
enb2<=1;
else
enb2<=0;
end
assign y=enb1|enb2;
endmodule

test bench:
module f_50tb1;
reg clk,rst;
wire [2:0]count;
wire enb1,enb2,y;
f5_50 dut(clk,rst,count,y,enb1,enb2);
initial begin
{clk,rst}=0;
end
always #5 clk=~clk;
initial begin
rst=1;
#10;
rst=0;
end
endmodule



